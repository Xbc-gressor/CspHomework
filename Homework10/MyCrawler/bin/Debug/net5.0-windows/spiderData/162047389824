/* 鼠标特效 */
var a_idx = 0;
jQuery(document).ready(function($) {
    $("body").click(function(e) {
        var a = new Array("free -h=查看内存","lsblk=查看系统的磁盘","df=查看文件系统硬盘使用情况","touch=创建文件","mkdir=创建文件夹","ls=列出文件","echo=输出出字符串或者打印变量值","pwd=显示当前路径","mv=移动或者更名","fdisk=磁盘分区","uname=显示系统信息","history=显示历史记录","zip=压缩文件","uzip=解压缩文件");
        var $i = $("<span/>").text(a[a_idx]);
        a_idx = (a_idx + 1) % a.length;
        var x = e.pageX,
        y = e.pageY;
        $i.css({
            "z-index": 999999999999999999999999999999999999999999999999999999999999999999999,
            "top": y - 20,
            "left": x,
            "position": "absolute",
            "font-weight": "bold",
            "color": "rgb(72,85,137)"
        });
        $("body").append($i);
        $i.animate({
            "top": y - 180,
            "opacity": 0
        },
        1500,
        function() {
            $i.remove();
        });
    });
});
</script>
