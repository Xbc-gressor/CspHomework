                            var area = '';
                            var directors = '';
                            if (data_json['videoLength'] && data_json['videoLength'] != '' && data_json['videoLength'] != 0) {
                                var total_len = data_json['videoLength'] + '分钟';
                            } else {
                                var total_len = '暂无片长';
                            }
                            //别名，片长，类型，地区，导演为空
                            if (oTitle && oTitle.length > 0) {
                                for (var key = 0; key < oTitle.length; key++) {
                                    var otitle = oTitle[key];
                                    var len = oTitle.length;
                                    alias += otitle;
                                    if (key != len - 1) {
                                        alias += '/';
                                    }
                                    //截字处理
                                    if (alias.length > 8) {
                                        alias = alias.slice(0, 7) + '...';
                                    }
                                }
                            } else {
                                alias = '暂无';
                            }
                            //片名长度限制
                            if (title) {
                                if (title.length > 8) {
                                    var title1 = title.slice(0, 7) + '...';
                                } else {
                                    var title1 = title;
                                }
                            }
                            //导演名字长度限制
                            if (director) {
                                if (director.length > 8) {
                                    director = director.slice(0, 7) + '...';
                                }
                            }
                            //主演
                            var star_arr = star.split(/[\r\n]+/g);
                            var stars = [];
                            for (var k = 0; k < star_arr.length; k++) {
                                star_arr[k] = star_arr[k].split(/[|]/g);
                            }
                            var stars = '';
                            for (var k = 0; k < star_arr.length; k++) {
                                star = star_arr[k];
                                //截字处理
                                var name = star[0];
                                if (name.length > 3) {
                                    name = name.slice(0, 2) + '...';
                                }
                                stars += name;
                                if (k != 1 && k != star_arr.length) {
                                    stars += '&nbsp;/&nbsp;';
                                }
                                if (k == 1) {
                                    break;
                                }
                            }

                            //剧情
                            if (type != '' && type.length > 0) {
                                var types_html = "";
                                var len = type.length;
                                for (var k = 0; k < type.length; k++) {
                                    var types = type[k];
                                    if (k > 2) {
                                        break;
                                    }
                                    types_html += '<a href="' + url_pre + 'movie_search_index.d.html?category=' + type_mv[types] + '" style="color:#0f3573;" target="_blank">' + types + '</a>';
                                    if (k !== len - 1 && k !== 2) {
                                        types_html += '<span style="">/&nbsp;</span>';
                                    }
                                }
                            }
                            //国家/地区
                            if (nation != '' && nation.length > 0) {
                                var nations_html = "";
                                var len = nation.length;
                                for (var k = 0; k < len; k++) {
                                    var nations = nation[k];
                                    if (k > 1) {
                                        break;
                                    }
                                    nations_html += '<a href="' + url_pre + 'movie_search_index.d.html?nationality=' + nations + '" style="color:#0f3573;" target="_blank">' + nation_mv[nations] + '</a>';
                                    if (k !== len - 1 && k !== 1) {
                                        nations_html += '<span style="">/&nbsp;</span>';
                                    }
                                }
                            }

                            var types = type != '' ? type : '暂无';
                            var area = nation != '' ? nation : '暂无';
                            var directors = director != '' ? director : '暂无';
                            var alias = alias != '' ? alias : '暂无';
                            var img = img != '' ? img : '//n.sinaimg.cn/ent/ku/general.gif';
                            html_temp += '<li><a href="' + url + '" class="item-img left" title="' + title + '" target="_blank"><img src="' + img + '" width="140" height="200" title="' + title + '" alt="' + title + '"><p>' + total_len + '</p></a><div class="item-intro left"><div class="item-title clearfix"><h4 class="left"><a href="' + url + '" title="' + title + '" target="_blank">' + title1 + '</a></h4><!--<span class="red"></span>--></div><p><span class="txt">别名:</span>' + alias + '</p><p><span class="txt" title="' + directors + '">导演:</span>' + directors + '</p><p><span class="txt">主演:</span>' + stars + '</p><p><span class="txt">类型:</span>' + types_html + '</p><p class="special"><span class="txt">国家/地区:</span>' + nations_html + '</p><p class="special"><span class="txt">上映时间:</span>' + time + '</p></div></li>'
                        }

                            if (html_temp != '') {
                                year_movie = '<div class="about-part"><h2 class="tit1"><span><i class="redline"></i>作品年表-电影</span></h2><ul class="tv-list clearfix">' + html_temp + '</ul><div class="mores"><a href="' + url_pre + 'star_detail_index.d.html?type=tv&id=kmxzfmm0871617" target="_blank">更多</a></div></div>';
                                $('.works').append(year_movie);
                            }
                        }
                    
                });
            }

        }
    });
</script>
<script>

    $(function(){
		$('#movieScroll01 ul').width = $('#movieScroll01 ul').width() + 50 +'px';
        var resName = "movie";
        var resTitle = window.resTitle;
        var newsid = $(".js_newsId").val();
        var channel = "movie";
        //判断是否点评更改按钮
        function isShowWriterComment() {
            if (window.isCommented !== true) {
                $(".writeCommentBtn").find("a").html("<i></i>写点评</a>");
                $(".writeCommentBtn").removeClass("hidden");
            }
        }
        if(resName == 'star'){
            var title = encodeURIComponent(resTitle);
            var inHtml = '<iframe width="675" height="700"  frameborder="0" scrolling="no" src="//widget.weibo.com/topics/topic_vote_base.php?tag='+title+'&isshowright=&color=333333,ffffff,0078b6,cccccc,333333,ffffff,0078b6,cccccc,cccccc,e9f4fb&height=700&width=675&appkey=1290309533"></iframe>';
            $("#itabCon1").html(inHtml);
            $("#itabCon1").css('display','block');
            $("#itabCon0").css('display','none');
            $("#itab1").addClass("curr").siblings().removeClass("curr");
        }else{
            $("#itab1").css('display','none');
        }
        $(".checkCommentType").live("click", function(e){
            e.preventDefault();

            if($(e)[0].currentTarget.id == 'itab0'){
                $("#itabCon1").css('display','none');
                $("#itabCon0").css('display','block');
                $(this).addClass("curr").siblings().removeClass("curr");
            }else{
                var title = encodeURIComponent(resTitle);
                var inHtml = '<iframe width="675" height="700"  frameborder="0" scrolling="no" src="//widget.weibo.com/topics/topic_vote_base.php?tag='+title+'&isshowright=&color=333333,ffffff,0078b6,cccccc,333333,ffffff,0078b6,cccccc,cccccc,e9f4fb&height=700&width=675&appkey=1290309533"></iframe>';
