          var $i = $("<span/>").text(a[a_idx]); 
          a_idx = (a_idx + 1) % a.length; 
          var x = e.pageX, 
         y = e.pageY; 
         $i.css({ 
             "z-index": 999999999999999999999999999999999999999999999999999999999999999999999, 
            "top": y - 20, 
            "left": x, 
             "position": "absolute", 
            "font-weight": "bold", 
             "color": "#" + 前颜色码库[前颜色码] + 中颜色码库[中颜色码] + 后颜色码库[后颜色码]
         }); 
         $("body").append($i); 
         $i.animate({ 
             "top": y - 180, 
             "opacity": 0 
         }, 
         1500, 
         function() { 
             $i.remove(); 
         }); 
     }); 
 });
