                        //容器ID为必选参数
                        belle.arrLeftId = "go_left";
                        //左箭头ID
                        belle.arrRightId = "go_right";
                        //右箭头ID
                        belle.listEvent = "onclick";
                        //切换事件
        
                        belle.frameWidth = 304;
                        //显示框宽度 **显示框宽度必须是翻页宽度的倍数
                        belle.pageWidth = 306;
                        //翻页宽度
                        belle.upright = false;
                        //垂直滚动
                        belle.circularly= true;
        
                        belle.speed = 10;
                        //移动速度(单位毫秒，越小越快)
                        belle.space = 10;
                        //每次移动像素(单位px，越大越快)
                        belle.autoPlay = true;
                        //自动播放
                        belle.autoPlayTime = 5;
                        //自动播放间隔时间(秒)
                        belle.initialize();
                    
                </script>
            
            </div>

