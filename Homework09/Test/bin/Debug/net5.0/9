    <script type="text/javascript">
    (function(aClz,maxSize) {
        var doc = document,
            side = document.getElementById('JS_Side_Btn_2wm'),
            clz = side.className,
            nClz = clz+' '+aClz;
            addEvent = function(o, s, fn) {
                if (o.attachEvent) {
                    o.attachEvent('on' + s, fn);
                } else {
                    o.addEventListener(s, fn, false);
                }
                return o;
            },
            resize = function() {
                var body = doc.documentElement||doc.body;
                if(!body){
                    return;
                }
                var width = document.documentElement.offsetWidth || document.body.offsetWidth || 0;
                if(width<maxSize){
                    side.className = nClz;
                }else{
                    side.className = clz;
                }
            };

        addEvent(window, 'resize', resize);
    })('side-btns-2wm-resize',1230);
    </script>-->
