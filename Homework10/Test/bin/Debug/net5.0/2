                                    for (var key = 0; key < oTitle.length; key++) {
                                        var otitle = oTitle[key];
                                        var len = oTitle.length;
                                        alias += otitle;
                                        if (key != len - 1) {
                                            alias += '/';
                                        }
                                        //截字处理
                                        if (alias.length > 8) {
                                            alias = alias.slice(0, 7) + '...';
                                        }
                                    }
                                } else {
                                    alias = '暂无';
                                }
                                //片名长度限制
                                if (title) {
                                    if (title.length > 8) {
                                        var title1 = title.slice(0, 7) + '...';
                                    } else {
                                        var title1 = title;
                                    }
                                }
                                //导演名字长度限制
                                if (director) {
                                    if (director.length > 8) {
                                        director = director.slice(0, 7) + '...';
                                    }
                                }
                                //主演
                                var star_arr = star.split(/[\r\n]+/g);
                                var stars = [];
                                for (var k = 0; k < star_arr.length; k++) {
                                    star_arr[k] = star_arr[k].split(/[|]/g);
                                }
                                var stars = '';
                                for (var k = 0; k < star_arr.length; k++) {
                                    star = star_arr[k];
                                    //截字处理
                                    $strnum = 3;
                                    //注意词之间有空格间隔
                                    var name = star[0];
                                    if (name.length > 3) {
                                        name = name.slice(0, 2) + '...';
                                    }
                                    stars += name;
                                    if (k != 1 && k != star_arr.length) {
                                        stars += '&nbsp;/&nbsp;';
                                    }
                                    if (k == 1) {
                                        break;
                                    }
                                }

                                //剧情
                                if (type != '' && type.length > 0) {
                                    var types_html = "";
                                    var len = type.length;
                                    for (var k = 0; k < type.length; k++) {
                                        var types = type[k];
                                        if (k > 2) {
                                            break;
                                        }
                                        types = types.replace(/(^\s*)|(\s*$)/g, "");
                                        types_html += '<a href="' + url_pre + 'movie_search_index.d.html?category=' + type_tv[types] + '" style="color:#0f3573;" target="_blank">' + types + '</a>';
                                        if (k !== len - 1 && k !== 2) {
                                            types_html += '<span style="">/&nbsp;</span>';
                                        }
                                    }
                                }
                                //国家/地区
                                if (nation != '' && nation.length > 0) {
                                    var nations_html = "";
                                    var len = nation.length;
                                    for (var k = 0; k < len; k++) {
                                        var nations = nation[k];
                                        if (k > 1) {
                                            break;
